graph TD
    %% SynergyMesh Governance Framework - Dependency Map
    %% Version: 1.0.0
    
    %% Strategic Layer
    V[00-vision-strategy<br/>Vision & Strategy]
    A[01-architecture<br/>Architecture]
    
    %% Governance Layer
    D[02-decision<br/>Decision]
    C[03-change<br/>Change]
    R[04-risk<br/>Risk]
    CP[05-compliance<br/>Compliance]
    
    %% Implementation Layer
    S[06-security<br/>Security]
    AU[07-audit<br/>Audit]
    P[08-process<br/>Process]
    PF[09-performance<br/>Performance]
    ST[82-stakeholder<br/>Stakeholder]
    
    %% Support Layer
    T[11-tools-systems<br/>Tools & Systems]
    CU[12-culture-capability<br/>Culture & Capability]
    M[13-metrics-reporting<br/>Metrics & Reporting]
    I[14-improvement<br/>Improvement]
    
    %% Innovation Layer
    E[15-economic<br/>Economic]
    PS[16-psychological<br/>Psychological]
    SO[17-sociological<br/>Sociological]
    CS[18-complex-system<br/>Complex Systems]
    EV[19-evolutionary<br/>Evolutionary]
    IN[20-information<br/>Information]
    EC[21-ecological<br/>Ecological]
    AE[22-aesthetic<br/>Aesthetic]
    
    %% Strategic Layer Dependencies
    V --> A
    V -.-> D
    V -.-> C
    V -.-> R
    V -.-> CP
    
    %% Governance Layer Dependencies
    A --> D
    A --> C
    A --> R
    A --> CP
    
    D --> C
    D --> R
    D --> P
    
    C --> R
    C --> CP
    
    R --> CP
    R --> S
    
    CP --> S
    CP --> AU
    
    %% Implementation Layer Dependencies
    S --> AU
    
    P --> PF
    PF --> M
    
    %% Support Layer (connects to all)
    T -.-> AU
    CU -.-> AU
    M -.-> AU
    I -.-> AU
    
    %% Innovation Layer Dependencies
    A --> E
    A --> PS
    A --> SO
    A --> CS
    A --> EV
    A --> IN
    A --> EC
    A --> AE
    
    ST -.-> PS
    ST -.-> SO
    
    I --> EV
    T --> IN
    
    %% Styling
    classDef strategic fill:#667eea,stroke:#333,stroke-width:2px,color:#fff
    classDef governance fill:#764ba2,stroke:#333,stroke-width:2px,color:#fff
    classDef implementation fill:#f093fb,stroke:#333,stroke-width:2px,color:#fff
    classDef support fill:#4facfe,stroke:#333,stroke-width:2px,color:#fff
    classDef innovation fill:#43e97b,stroke:#333,stroke-width:2px,color:#000
    
    class V,A strategic
    class D,C,R,CP governance
    class S,AU,P,PF,ST implementation
    class T,CU,M,I support
    class E,PS,SO,CS,EV,IN,EC,AE innovation
