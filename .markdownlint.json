{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "consistent"
  },
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "tables": false
  },
  "MD024": {
    "siblings_only": true
  },
  "MD025": true,
  "MD033": {
    "allowed_elements": ["div", "span", "br", "img", "sup", "sub", "details", "summary", "p", "a", "hr", "table", "thead", "tbody", "tr", "th", "td"]
  },
  "MD036": false,
  "MD040": true,
  "MD041": true,
  "MD046": {
    "style": "consistent"
  },
  "MD051": false,
  "MD055": false,
  "MD056": false,
  "MD060": false
}
